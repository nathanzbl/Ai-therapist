{
  "type": "module",
  "scripts": {
    "dev": "node src/server/index.js --dev",
    "start": "NODE_ENV=production node src/server/index.js",
    "build": "npm run build:client && npm run build:admin-client && npm run build:redact-client && npm run build:server && npm run build:admin-server && npm run build:redact-server",
    "build:client": "vite build --outDir ../../../dist/client --ssrManifest",
    "build:admin-client": "vite build --config vite.admin.config.js --outDir ../../../dist/admin-client --ssrManifest",
    "build:redact-client": "vite build --config vite.redact.config.js --outDir ../../../dist/redact-client --ssrManifest",
    "build:server": "vite build --ssr entry-server.jsx --outDir ../../../dist/server --emptyOutDir",
    "build:admin-server": "vite build --ssr admin-entry-server.jsx --config vite.admin.config.js --outDir ../../../dist/admin-server --emptyOutDir",
    "build:redact-server": "vite build --ssr redact-entry-server.jsx --config vite.redact.config.js --outDir ../../../dist/redact-server --emptyOutDir",
    "devinstall": "zx ../../devinstall.mjs -- node src/server/index.js --dev",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint -c .eslintrc.json . --ext .js,.jsx --fix"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-secrets-manager": "^3.857.0",
    "bcrypt": "^6.0.0",
    "connect-pg-simple": "^10.0.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.2",
    "geoip-lite": "^1.4.10",
    "history": "^5.3.0",
    "minipass": "^7.0.4",
    "openai": "^5.17.0",
    "openai-realtime-api": "^1.0.8",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-router-dom": "^6.20.0",
    "recharts": "^3.6.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "speakeasy": "^2.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "postcss": "^8.5.6",
    "postcss-nesting": "^12.0.2",
    "postcss-preset-env": "^7.7.1",
    "tailwindcss": "^3.4.17",
    "vite": "^6.2.0"
  }
}
